{
  "name": "clsfrutal-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker rm -f clf-postgres-dev 2>/dev/null || true && docker-compose up -d && next dev",
    "firebase": "firebase emulators:start --project clsfrutal --config=./firebase/firebase.json --import=./exportdatabase --export-on-exit",
    "init": "concurrently \"npm run dev\" \"npm run firebase\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:restart": "docker-compose restart",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "migrate": "node-pg-migrate",
    "migrate:create": "node-pg-migrate create",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "seed": "ts-node seeds/createAminUser.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.1",
    "firebase": "^12.0.0",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "next": "^16.0.7",
    "next-connect": "^1.0.0",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.0",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.17.31",
    "@types/pg": "^8.11.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.9.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
